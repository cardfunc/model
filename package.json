{
 "name": "@payfunc/model-card",
 "version": "0.2.5",
 "description": "Javascript library containing the model for the PayFunc Card Tokenizing REST API.",
 "author": "PayFunc",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "git+https://github.com/payfunc/model-card"
 },
 "bugs": {
  "url": "https://github.com/payfunc/model-card/issues"
 },
 "homepage": "https://github.com/payfunc/model-card#readme",
 "private": false,
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "transform": {
   "^.+\\.(j|t)sx?$": "ts-jest"
  },
  "transformIgnorePatterns": [
   "<rootDir>/node_modules/(?!(cryptly|authly|isoly|gracely)/.*)"
  ],
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "eslint '**/*.{js,ts,tsx}'",
  "fix": "eslint '**/*.{js,ts,tsx}' --fix",
  "build": "tsc -p .",
  "test": "jest --maxWorkers=2",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "authly": "0.2.2",
  "gracely": "0.0.39",
  "isoly": "0.0.26"
 },
 "devDependencies": {
  "@babel/core": "^7.11.0",
  "@types/jest": "^26.0.0",
  "@types/node": "^14.0.13",
  "@typescript-eslint/eslint-plugin": "^3.10.1",
  "@typescript-eslint/parser": "^3.10.1",
  "babel-jest": "^26.0.1",
  "eslint": "^7.7.0",
  "eslint-config-standardize": "^0.7.1",
  "eslint-plugin-prettierx": "github:nilssonemma/eslint-plugin-prettierx#master",
  "eslint-plugin-simple-import-sort": "^7.0.0",
  "jest": "26.6.3",
  "ts-jest": "^26.1.0",
  "typescript": "^3.9.5",
  "watch": "^1.0.2"
 }
}
